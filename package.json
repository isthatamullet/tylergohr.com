{
  "name": "tylergohr.com",
  "version": "1.0.0",
  "private": true,
  "description": "High-end creative portfolio website showcasing full-stack development mastery",
  "author": "Tyler Gohr",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "npm run copy-blog-assets && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:portfolio": "playwright test e2e/portfolio-redesign.spec.ts",
    "test:e2e:navigation": "playwright test e2e/navigation-comprehensive.spec.ts",
    "test:e2e:api": "playwright test e2e/api-integration.spec.ts",
    "test:e2e:visual": "playwright test --grep \"visual consistency\"",
    "test:e2e:performance": "playwright test --grep \"performance|Core Web Vitals\"",
    "test:e2e:accessibility": "playwright test --grep \"accessibility\"",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:update-screenshots": "playwright test --update-snapshots",
    "test:e2e:dev": "SKIP_VISUAL=true playwright test --grep \"accessibility|navigation|contact\" --reporter=line",
    "test:e2e:smoke": "FAST_MODE=true playwright test e2e/portfolio-redesign.spec.ts --grep \"Navigation dropdown content|Enterprise content validation\" --reporter=line",
    "test:e2e:screenshot": "playwright test e2e/screenshot-generation.spec.ts",
    "test:e2e:screenshot:mobile": "playwright test e2e/screenshot-generation.spec.ts --grep \"mobile\"",
    "test:e2e:claude-review": "playwright test e2e/screenshot-generation.spec.ts --grep \"Claude review\"",
    "test:e2e:claude-review:current": "playwright test e2e/screenshot-generation.spec.ts --grep \"current state capture\"",
    "test:e2e:preview": "playwright test e2e/screenshot-generation.spec.ts --grep \"quick preview\"",
    "test:e2e:component": "playwright test --grep",
    "test:e2e:dev:verbose": "SKIP_VISUAL=true playwright test --reporter=html",
    "test:e2e:debug": "playwright test --headed --timeout=0",
    "copy-blog-assets": "node scripts/copy-blog-assets.js",
    "validate": "npm run typecheck && npm run lint && npm run build",
    "pre-commit": "npm run validate"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@modelcontextprotocol/server-puppeteer": "^2025.5.12",
    "@monaco-editor/react": "^4.7.0",
    "@next/mdx": "^15.3.5",
    "@types/node": "^24.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "framer-motion": "^12.19.1",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.12",
    "monaco-editor": "^0.52.2",
    "next": "^15.3.4",
    "nodemailer": "^7.0.3",
    "prism-react-renderer": "^2.4.1",
    "puppeteer": "^24.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@playwright/test": "^1.53.1",
    "@types/nodemailer": "^6.4.17",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "lighthouse": "^12.7.0",
    "svgo": "^4.0.0"
  }
}
