{
  "name": "tylergohr.com",
  "version": "1.0.0",
  "private": true,
  "description": "High-end creative portfolio website showcasing full-stack development mastery",
  "author": "Tyler Gohr",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "npm run copy-blog-assets && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:portfolio": "playwright test e2e/portfolio-redesign.spec.ts",
    "test:e2e:navigation": "playwright test e2e/navigation-comprehensive.spec.ts",
    "test:e2e:api": "playwright test e2e/api-integration.spec.ts",
    "test:e2e:visual": "playwright test --grep \"visual consistency\"",
    "test:e2e:performance": "playwright test --grep \"performance|Core Web Vitals\"",
    "test:e2e:accessibility": "playwright test --grep \"accessibility\"",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:update-screenshots": "playwright test --update-snapshots",
    "copy-blog-assets": "node scripts/copy-blog-assets.js",
    "validate": "npm run typecheck && npm run lint && npm run build",
    "pre-commit": "npm run validate"
  },
  "dependencies": {
    "@modelcontextprotocol/server-puppeteer": "^2025.5.12",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "date-fns": "^4.1.0",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "framer-motion": "^12.19.1",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.12",
    "next": "^15.3.4",
    "nodemailer": "^7.0.3",
    "prism-react-renderer": "^2.4.1",
    "puppeteer": "^24.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/nodemailer": "^6.4.17",
    "@types/testing-library__jest-dom": "^5.14.9",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "lighthouse": "^12.7.0"
  }
}
